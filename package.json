{
  "name": "uglybot",
  "version": "1.0.0",
  "description": "an uglybot to test stuff out",
  "source": ["index.ts","./utils/replaceReadmeContent.ts"],
  "targets": {
    "default": {
      "sourceMap": {
        "inline": false
      },
      "context": "node",
      "distDir": "./build",
      "isLibrary": true,
      "optimize": false,
      "outputFormat": "esmodule",
      "engines": {
        "node": ">= 16"
      }
    }
  },
  "scripts": {
    "start": "run-func build/index.js main",
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:badge": "coverage-badge-creator --config './badge-config.json'",
    "coverage:replace": "run-func build/utils/replaceReadmeContent.js coverageReplace",
    "updateReadmeCoverage": "npm run coverage:replace && npm run coverage:badge"
  },
  "author": "dasith kuruppu",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "coverage-badge-creator": "^1.0.13",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "fast-redact": "^3.1.2",
    "run-func": "^3.0.0",
    "winston": "^3.8.1",
    "replace-in-file": "^6.3.5"
  },
  "engines": {
    "node": ">= 16"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.22.1",
    "parcel": "^2.7.0",
    "vitest": "^0.22.1"
  }
}